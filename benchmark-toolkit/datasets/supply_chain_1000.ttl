# === Multi-hop traceability example (10 hops) ===
# BATCH017 goes through complete supply chain with multiple transformations

ex:Product/BATCH017 a ex:Product ;
    ex:productType "Tomato" ;
    ex:batchId "BATCH017" ;
    trace:hasProducer ex:Producer/Farm001 ;
    ex:productionDate "2024-01-09T05:00:00"^^xsd:dateTime ;
    perm:hasPermission perm:Public .

ex:Transaction/TX017 a ex:Transaction ;
    trace:transactionDate "2024-01-10T06:00:00"^^xsd:dateTime ;
    trace:quantity 200.0 ;
    ex:from ex:Producer/Farm001 ;
    ex:to ex:Processor/Packing001 ;
    ex:product ex:Product/BATCH017 .

# Hop 2: Processing
ex:Product/BATCH017-PROCESSED a ex:Product ;
    ex:productType "Processed Tomato" ;
    ex:batchId "BATCH017-PROCESSED" ;
    ex:derivedFrom ex:Product/BATCH017 ;
    trace:processedBy ex:Processor/Packing001 ;
    ex:processingDate "2024-01-10T08:00:00"^^xsd:dateTime .

# Hop 3: To Distributor
ex:Transaction/TX017-2 a ex:Transaction ;
    trace:transactionDate "2024-01-11T07:00:00"^^xsd:dateTime ;
    trace:quantity 195.0 ;
    ex:from ex:Processor/Packing001 ;
    ex:to ex:Distributor/Dist001 ;
    ex:product ex:Product/BATCH017-PROCESSED .

# Hop 4: Distribution center storage
ex:Product/BATCH017-DIST a ex:Product ;
    ex:productType "Processed Tomato" ;
    ex:batchId "BATCH017-DIST" ;
    ex:derivedFrom ex:Product/BATCH017-PROCESSED ;
    trace:distributedBy ex:Distributor/Dist001 ;
    ex:storageDate "2024-01-11T10:00:00"^^xsd:dateTime .

# Hop 5: To Regional Distribution
ex:Transaction/TX017-3 a ex:Transaction ;
    trace:transactionDate "2024-01-12T06:00:00"^^xsd:dateTime ;
    trace:quantity 190.0 ;
    ex:from ex:Distributor/Dist001 ;
    ex:to ex:Distributor/Dist002 ;
    ex:product ex:Product/BATCH017-DIST .

# Hop 6: Quality Check
ex:Product/BATCH017-QC a ex:Product ;
    ex:productType "Certified Processed Tomato" ;
    ex:batchId "BATCH017-QC" ;
    ex:derivedFrom ex:Product/BATCH017-DIST ;
    ex:qualityCheck "PASSED" ;
    ex:qualityCheckDate "2024-01-12T12:00:00"^^xsd:dateTime .

# Hop 7: To Retailer
ex:Transaction/TX017-4 a ex:Transaction ;
    trace:transactionDate "2024-01-13T07:30:00"^^xsd:dateTime ;
    trace:quantity 185.0 ;
    ex:from ex:Distributor/Dist002 ;
    ex:to ex:Retailer/Retail001 ;
    ex:product ex:Product/BATCH017-QC .

# Hop 8: Retailer Inventory
ex:Product/BATCH017-RETAIL a ex:Product ;
    ex:productType "Certified Processed Tomato" ;
    ex:batchId "BATCH017-RETAIL" ;
    ex:derivedFrom ex:Product/BATCH017-QC ;
    trace:soldBy ex:Retailer/Retail001 ;
    ex:inventoryDate "2024-01-13T09:00:00"^^xsd:dateTime .

# Hop 9: Price Marking
ex:Product/BATCH017-PRICED a ex:Product ;
    ex:productType "Certified Processed Tomato" ;
    ex:batchId "BATCH017-PRICED" ;
    ex:derivedFrom ex:Product/BATCH017-RETAIL ;
    ex:price "45.50"^^xsd:decimal ;
    ex:currency "THB" ;
    ex:pricingDate "2024-01-13T10:00:00"^^xsd:dateTime .

# Hop 10: Ready for Sale
ex:Transaction/TX017-5 a ex:Transaction ;
    trace:transactionDate "2024-01-13T12:00:00"^^xsd:dateTime ;
    trace:quantity 180.0 ;
    ex:status "FOR_SALE" ;
    ex:product ex:Product/BATCH017-PRICED .

