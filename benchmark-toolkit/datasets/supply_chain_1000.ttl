# ProvChain-Org Benchmark Dataset: Supply Chain Traceability
# Format: RDF/Turtle (TTL)
# Description: 1000 triples representing food supply chain with provenance tracking
# Use Case: Multi-hop traceability query testing

@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/supplychain/> .
@prefix food: <http://example.org/food/> .
@prefix trace: <http://example.org/traceability#> .
@prefix perm: <http://example.org/permissions#> .

# === Ontology Definitions ===

ex:Producer a rdfs:Class ;
    rdfs:label "Producer"@en ;
    rdfs:comment "Entity that produces raw materials or products" .

ex:Processor a rdfs:Class ;
    rdfs:label "Processor"@en ;
    rdfs:comment "Entity that processes or transforms products" .

ex:Distributor a rdfs:Class ;
    rdfs:label "Distributor"@en ;
    rdfs:comment "Entity that distributes products to retailers" .

ex:Retailer a rdfs:Class ;
    rdfs:label "Retailer"@en ;
    rdfs:comment "Entity that sells products to consumers" .

ex:Product a rdfs:Class ;
    rdfs:label "Product"@en ;
    rdfs:comment "Product or batch in supply chain" .

ex:Transaction a rdfs:Class ;
    rdfs:label "Transaction"@en ;
    rdfs:comment "Transaction event in supply chain" .

trace:hasProducer a rdf:Property ;
    rdfs:domain ex:Product ;
    rdfs:range ex:Producer .

trace:processedBy a rdf:Property ;
    rdfs:domain ex:Product ;
    rdfs:range ex:Processor .

trace:distributedBy a rdf:Property ;
    rdfs:domain ex:Product ;
    rdfs:range ex:Distributor .

trace:soldBy a rdf:Property ;
    rdfs:domain ex:Product ;
    rdfs:range ex:Retailer .

trace:hasTransaction a rdf:Property ;
    rdfs:domain ex:Product ;
    rdfs:range ex:Transaction .

trace:transactionDate a rdf:Property ;
    rdfs:domain ex:Transaction ;
    rdfs:range xsd:dateTime .

trace:quantity a rdf:Property ;
    rdfs:domain ex:Transaction ;
    rdfs:range xsd:decimal .

perm:hasPermission a rdf:Property ;
    rdfs:domain ex:Product ;
    rdfs:range perm:Permission .

perm:Permission a rdfs:Class .
perm:Public a perm:Permission .
perm:Private a perm:Permission .
perm:Restricted a perm:Permission .

